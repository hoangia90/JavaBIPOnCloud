<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	
	<servlet>
	    <servlet-name>Compute</servlet-name>
	    <servlet-class>compute.Compute</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>Monitor</servlet-name>
		<servlet-class>monitor.Monitor</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>AppUser</servlet-name>
		<servlet-class>user.RandomUser</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>Test</servlet-name>
		<servlet-class>test</servlet-class>
	</servlet>
	
	<servlet-mapping>
	    <servlet-name>Compute</servlet-name>
	    <url-pattern>/req</url-pattern>
	</servlet-mapping>
 
	<servlet-mapping>
		<servlet-name>Monitor</servlet-name>
		<url-pattern>/moni</url-pattern>
	</servlet-mapping>
 
	<servlet-mapping>
		<servlet-name>AppUser</servlet-name>
		<url-pattern>/usr</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Test</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	
	
</web-app>
